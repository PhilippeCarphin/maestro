export ORDSOUMET_date_started="$(LC_TIME=C date)"
export ORDSOUMET_date_submitted="Fri Apr 17 18:10:10 UTC 2020"
export ORDSOUMET_date_begin_ordsoumet="Fri Apr 17 18:10:10 UTC 2020"
export ORDSOUMET_date_prepared="Fri Apr 17 18:10:10 UTC 2020"
export ORDSOUMET_date_submit_done=""

echo shredderTask.+0:0: JOB_ORDSOUMET at "$ORDSOUMET_date_begin_ordsoumet" Name=shredderTask.+0 Dest=eccc-ppp3 CPUS=1 SYSTEM_ID="${JOB_ID:-${LOADL_STEP_ID%.*}}"
echo shredderTask.+0:1: JOB_PREPARED at "$ORDSOUMET_date_prepared" Name=shredderTask.+0 Dest=eccc-ppp3 CPUS=1 SYSTEM_ID="${JOB_ID:-${LOADL_STEP_ID%.*}}"
echo shredderTask.+0:2: JOB_SUBMITTED at "$ORDSOUMET_date_submitted" Name=shredderTask.+0 Dest=eccc-ppp3 CPUS=1 SYSTEM_ID="${JOB_ID:-${LOADL_STEP_ID%.*}}"
echo shredderTask.+0:3: JOB_SUBMIT_DONE at "$ORDSOUMET_date_submit_done" Name=shredderTask.+0 Dest=eccc-ppp3 CPUS=1 SYSTEM_ID=""
echo shredderTask.+0:4: JOB_RUNWRAPPER at "$ORDSOUMET_date_runwrapper" Name=shredderTask.+0 Dest=eccc-ppp3 CPUS=1 SYSTEM_ID="${JOB_ID:-${LOADL_STEP_ID%.*}}"
echo shredderTask.+0:5: JOB_STARTED at "$ORDSOUMET_date_started" Name=shredderTask.+0 Dest=eccc-ppp3 CPUS=1 SYSTEM_ID="${JOB_ID:-${LOADL_STEP_ID%.*}}"

export JobStartTime=$(date +%s -d "$ORDSOUMET_date_started")
export JobTimeLimit=300
export OMP_NUM_THREADS=1

# temporaire tant que je n'aurai pas trouvé de solution homogène...
if [[ "PBSPro" != "LoadLeveler5" ]]; then
	((OMP_NUM_THREADS=OMP_NUM_THREADS*1))
fi
export BATCH_MPI_CPUS=1

if [ -n "" ]; then
	export DISPLAY=
fi
if [ -n "" ]; then
	xterm -ls &
fi

nodelogger -n ${SEQ_NODE} ${SEQ_LOOP_ARGS} -s info -m "shredderTask seq_node=${SEQ_NODE} seq_loop_args=${SEQ_LOOP_ARGS} not_an_env_var='${BLAHBLAH}' exp_home='${SEQ_EXP_HOME}'"

set +x
ORDSOUMET_date_ended="$(LC_TIME=C date)"
echo shredderTask.+0:6: JOB_ENDED at "$ORDSOUMET_date_ended" WallTime=$(echo  $(date +%s -d "$ORDSOUMET_date_ended") - $JobStartTime | bc)s Name=shredderTask.+0 Dest=eccc-ppp3 CPUS=1 SYSTEM_ID="${JOB_ID:-${LOADL_STEP_ID%.*}}"
