<MODULE name="validation">
    <SUBMITS sub_name="champ_essai"/>
    <SUBMITS sub_name="analyses"/>
    <SUBMITS sub_name="precipitations"/>
    <SUBMITS sub_name="meteo_generale"/>
    <SUBMITS sub_name="aviation"/>
    <SUBMITS sub_name="temps_violent_hiver"/>
    <SUBMITS sub_name="temps_violent_ete"/>
    <SUBMITS sub_name="perfect_prog"/>
    <SUBMITS sub_name="divers"/>
    <SUBMITS sub_name="domaines"/>
    <SUBMITS sub_name="labCentral_tracks"/>
    <SUBMITS sub_name="labCentral_bourrasques"/>
    <SUBMITS sub_name="labHAL_fog_stratus"/>
    <FAMILY name="champ_essai">
        <SUBMITS sub_name="validation_plhaa"/>
        <MODULE name="validation_plhaa"/>
    </FAMILY>
    <FAMILY name="analyses">
        <SUBMITS sub_name="validation_fx250_fx500"/>
        <SUBMITS sub_name="validation_plhae"/>
        <SUBMITS sub_name="validation_plhj"/>
        <SUBMITS sub_name="validation_plhb"/>
        <SUBMITS sub_name="validation_aws_250mb"/>
        <SUBMITS sub_name="validation_aws_500mb"/>
        <MODULE name="validation_fx250_fx500"/>
        <MODULE name="validation_plhae"/>
        <MODULE name="validation_plhj"/>
        <MODULE name="validation_plhb"/>
        <MODULE name="validation_aws_250mb"/>
        <MODULE name="validation_aws_500mb"/>
    </FAMILY>
    <FAMILY name="precipitations">
        <SUBMITS sub_name="calculs"/>
        <SUBMITS sub_name="cartes"/>
        <SUBMITS sub_name="bulletins"/>
	<FAMILY name="calculs">
	  <SUBMITS sub_name="validation_pcpn_interval"/>
	  <SUBMITS sub_name="validation_pcpn_interval_native"/>
	  <MODULE name="validation_pcpn_interval"/>
	  <MODULE name="validation_pcpn_interval_native"/>
	</FAMILY>
        <FAMILY name="cartes">
            <SUBMITS sub_name="validation_plhfc"/>
	    <SUBMITS sub_name="run"/>
            <MODULE name="validation_plhfc"/>
	    <SWITCH name="run" type="datestamp_hour">
              <SWITCH_ITEM name="00">
		<SUBMITS sub_name="validation_plhcd"/>
		<SUBMITS sub_name="validation_plhce"/>
		<MODULE name="validation_plhcd"/>
		<MODULE name="validation_plhce"/>
	      </SWITCH_ITEM>
	    </SWITCH>
        </FAMILY>
        <FAMILY name="bulletins">
            <SUBMITS sub_name="validation_fgc13"/>
            <MODULE name="validation_fgc13"/>
        </FAMILY>
    </FAMILY>
    <FAMILY name="meteo_generale">
        <SUBMITS sub_name="calculs"/>
        <SUBMITS sub_name="cartes"/>
        <SUBMITS sub_name="bulletins"/>
	<FAMILY name="calculs">
	    <SUBMITS sub_name="validation_loop_humidity_and_temperature"/>
	    <MODULE name="validation_loop_humidity_and_temperature"/>
	</FAMILY>
        <FAMILY name="cartes">
            <SUBMITS sub_name="validation_dfx20"/>
            <SUBMITS sub_name="validation_loop_4_pan_classique"/>
            <SUBMITS sub_name="validation_2_panneaux_classique"/>
            <SUBMITS sub_name="validation_2_panneaux_classique_fichiers_standard"/>
            <SUBMITS sub_name="validation_dfxf4"/>
            <SUBMITS sub_name="validation_dfx24"/>
            <MODULE name="validation_dfx20"/>
            <MODULE name="validation_loop_4_pan_classique"/>
            <MODULE name="validation_2_panneaux_classique"/>
            <MODULE name="validation_2_panneaux_classique_fichiers_standard"/>
            <MODULE name="validation_dfxf4"/>
            <MODULE name="validation_dfx24"/>
        </FAMILY>
        <FAMILY name="bulletins">
            <SUBMITS sub_name="validation_fxcn6"/>
	    <SUBMITS sub_name="run"/>
            <MODULE name="validation_fxcn6"/>
	    <SWITCH name="run" type="datestamp_hour">
              <SWITCH_ITEM name="12">
		<SUBMITS sub_name="validation_fpext"/>
		<SUBMITS sub_name="validation_fpext_fichiers_standard"/>
		<MODULE name="validation_fpext"/>
		<MODULE name="validation_fpext_fichiers_standard"/>
	      </SWITCH_ITEM>
	    </SWITCH>
        </FAMILY>
    </FAMILY>
    <FAMILY name="aviation">
        <SUBMITS sub_name="cartes"/>
        <SUBMITS sub_name="bulletins"/>
        <FAMILY name="cartes">
            <SUBMITS sub_name="aviation_atlantique_nord"/>
            <SUBMITS sub_name="givrage"/>
