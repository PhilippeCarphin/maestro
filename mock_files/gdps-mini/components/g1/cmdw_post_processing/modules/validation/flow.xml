        <SUBMITS sub_name="validation_fxcn5_bulletins"/>
        <SUBMITS sub_name="validation_mupy2"/>
        <SUBMITS sub_name="validation_mupy1"/>
        <SUBMITS sub_name="jobs_a_00Z"/>
        <MODULE name="validation_ft123"/>
        <MODULE name="validation_ym372"/>
        <MODULE name="validation_pps72"/>
        <MODULE name="validation_pppcl"/>
        <MODULE name="validation_fxcn5_fichiers_standard"/>
        <MODULE name="validation_fxcn5_bulletins"/>
        <MODULE name="validation_mupy2"/>
        <MODULE name="validation_mupy1"/>
        <SWITCH name="jobs_a_00Z" type="datestamp_hour">
            <SWITCH_ITEM name="00">
                <SUBMITS sub_name="validation_ft35a"/>
                <MODULE name="validation_ft35a"/>
                <SUBMITS sub_name="validation_anefu"/>
                <MODULE name="validation_anefu"/>
                <SUBMITS sub_name="validation_anefu_bulletins"/>
                <MODULE name="validation_anefu_bulletins"/>
                <SUBMITS sub_name="validation_y3144"/>
                <MODULE name="validation_y3144"/>
                <SUBMITS sub_name="validation_pc144"/>
                <MODULE name="validation_pc144"/>
                <SUBMITS sub_name="validation_ps144"/>
                <MODULE name="validation_ps144"/>
                <SUBMITS sub_name="validation_ft710"/>
                <MODULE name="validation_ft710"/>
                <SUBMITS sub_name="validation_tfmcn"/>
                <MODULE name="validation_tfmcn"/>
            </SWITCH_ITEM>
            <SWITCH_ITEM name="12"/>
        </SWITCH>
    </FAMILY>
    <FAMILY name="divers">
        <SUBMITS sub_name="run"/>
        <SUBMITS sub_name="validation_uvboz_fichiers_standard"/>
        <SUBMITS sub_name="validation_uvboz_fichiers_burp"/>
        <SUBMITS sub_name="validation_dfxuv"/>
	<SWITCH name="run" type="datestamp_hour">
          <SWITCH_ITEM name="00">
            <SUBMITS sub_name="validation_plhca"/>
            <SUBMITS sub_name="validation_plhcb"/>
            <SUBMITS sub_name="validation_spwav"/>
            <MODULE name="validation_plhca"/>
            <MODULE name="validation_plhcb"/>
            <MODULE name="validation_spwav"/>	    
	  </SWITCH_ITEM>
	</SWITCH>
        <MODULE name="validation_uvboz_fichiers_standard"/>
        <MODULE name="validation_uvboz_fichiers_burp"/>
        <MODULE name="validation_dfxuv"/>
