                <SUBMITS sub_name="validation_loop_jt"/>
                <SUBMITS sub_name="validation_loop_ti"/>
                <SUBMITS sub_name="validation_plhsg"/>
                <SUBMITS sub_name="validation_loop_jt_fichiers_standard"/>
                <SUBMITS sub_name="validation_loop_ti_fichiers_standard"/>
