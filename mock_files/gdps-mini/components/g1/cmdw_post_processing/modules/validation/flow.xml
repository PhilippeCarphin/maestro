                <MODULE name="validation_loop_ti"/>
                <MODULE name="validation_plhsg"/>
                <MODULE name="validation_loop_jt_fichiers_standard"/>
                <MODULE name="validation_loop_ti_fichiers_standard"/>
            </FAMILY>
            <FAMILY name="givrage">
                <SUBMITS sub_name="validation_plhi"/>
                <MODULE name="validation_plhi"/>
            </FAMILY>
        </FAMILY>
        <FAMILY name="bulletins">
            <SUBMITS sub_name="validation_loop_fbcn"/>
            <SUBMITS sub_name="validation_fnt12"/>
            <MODULE name="validation_loop_fbcn"/>
            <MODULE name="validation_fnt12"/>
        </FAMILY>
    </FAMILY>
    <FAMILY name="temps_violent_hiver">
        <SUBMITS sub_name="calculs"/>
        <FAMILY name="calculs">
            <SUBMITS sub_name="validation_loop_edigraf"/>
            <SUBMITS sub_name="validation_loop_winter_severe_weather"/>
            <MODULE name="validation_loop_edigraf"/>
            <MODULE name="validation_loop_winter_severe_weather"/>
        </FAMILY>
    </FAMILY>
    <FAMILY name="temps_violent_ete">
        <SUBMITS sub_name="calculs"/>
        <FAMILY name="calculs">
            <SUBMITS sub_name="validation_loop_convective_indices_buoyancy"/>
            <SUBMITS sub_name="validation_loop_capes"/>
            <MODULE name="validation_loop_convective_indices_buoyancy"/>
            <MODULE name="validation_loop_capes"/>
        </FAMILY>
    </FAMILY>
    <FAMILY name="perfect_prog">
        <SUBMITS sub_name="validation_ft123"/>
        <SUBMITS sub_name="validation_ym372"/>
        <SUBMITS sub_name="validation_pps72"/>
        <SUBMITS sub_name="validation_pppcl"/>
