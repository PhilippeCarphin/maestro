<NODE_RESOURCES>
  <BATCH machine="${FRONTEND}" memory="1G" mpi="0" wallclock="10" catchup="4" cpu="1" queue="${FRONTEND_DEFAULT_Q}"/>
  <DEPENDS_ON dep_name="/main/intxfer/intxfer_anl/interp_anlpres" status="end" type="node" exp="${CMDW_FULL_G1_EXP}"/>
  <DEPENDS_ON dep_name="/main/intxfer/intxfer_prog/loop_prog/interp_progpres" index="loop_prog=60" status="end" type="node" exp="${CMDW_FULL_G1_EXP}"/>
  <DEPENDS_ON dep_name="/main/intxfer/intxfer_prog/loop_prog/interp_progpres" index="loop_prog=48" status="end" type="node" exp="${CMDW_FULL_G1_EXP}"/>
  <DEPENDS_ON dep_name="/main/intxfer/intxfer_prog/loop_prog/interp_progpres" index="loop_prog=36" status="end" type="node" exp="${CMDW_FULL_G1_EXP}"/>
  <DEPENDS_ON dep_name="/main/intxfer/intxfer_prog/loop_prog/interp_progpres" index="loop_prog=24" status="end" type="node" exp="${CMDW_FULL_G1_EXP}"/>
  <DEPENDS_ON dep_name="/main/intxfer/intxfer_prog/loop_prog/interp_progpres" index="loop_prog=12" status="end" type="node" exp="${CMDW_FULL_G1_EXP}"/>

  <!-- sfmbc is running only a 18Z, this dependency exist only at 00Z -->
  <DEPENDS_ON dep_name="/main/cmdw_post_processing/perfect_prog/sfmbc" index="perfect_prog=18" valid_hour="00" status="end" type="node" exp="${CMDW_POST_PROC_G6_EXP}" hour="-6"/>
  
  <!-- regional -->
  <DEPENDS_ON dep_name="/main/cmdw_post_processing/observations/synop" status="end" type="node" exp="${CMDW_POST_PROC_R1_EXP}" />
  <DEPENDS_ON dep_name="/main/cmdw_post_processing/perfect_prog/run/ft12x" index="run=00" status="end" type="node" exp="${CMDW_POST_PROC_R1_EXP}" valid_hour="00"/>
  <DEPENDS_ON dep_name="/main/cmdw_post_processing/perfect_prog/run/ft12x" index="run=12" status="end" type="node" exp="${CMDW_POST_PROC_R1_EXP}" valid_hour="12"/>

 <!-- g1 -->
  <DEPENDS_ON dep_name="/main/cmdw_post_processing/perfect_prog/jobs_a_00Z/ft35a" index="jobs_a_00Z=00" status="end" type="node" valid_hour="12"  hour="-12"/>
  <DEPENDS_ON dep_name="/main/cmdw_post_processing/perfect_prog/ft123" status="end" type="node" valid_hour="00"  hour="-12"/>
</NODE_RESOURCES>
