export ORDSOUMET_date_started="$(LC_TIME=C date)"
export ORDSOUMET_date_submitted="Fri Apr 17 18:10:33 UTC 2020"
export ORDSOUMET_date_begin_ordsoumet="Fri Apr 17 18:10:33 UTC 2020"
export ORDSOUMET_date_prepared="Fri Apr 17 18:10:33 UTC 2020"
export ORDSOUMET_date_submit_done=""

echo turtleTask2:0: JOB_ORDSOUMET at "$ORDSOUMET_date_begin_ordsoumet" Name=turtleTask2 Dest=eccc-ppp3 CPUS=1 SYSTEM_ID="${JOB_ID:-${LOADL_STEP_ID%.*}}"
echo turtleTask2:1: JOB_PREPARED at "$ORDSOUMET_date_prepared" Name=turtleTask2 Dest=eccc-ppp3 CPUS=1 SYSTEM_ID="${JOB_ID:-${LOADL_STEP_ID%.*}}"
echo turtleTask2:2: JOB_SUBMITTED at "$ORDSOUMET_date_submitted" Name=turtleTask2 Dest=eccc-ppp3 CPUS=1 SYSTEM_ID="${JOB_ID:-${LOADL_STEP_ID%.*}}"
echo turtleTask2:3: JOB_SUBMIT_DONE at "$ORDSOUMET_date_submit_done" Name=turtleTask2 Dest=eccc-ppp3 CPUS=1 SYSTEM_ID=""
echo turtleTask2:4: JOB_RUNWRAPPER at "$ORDSOUMET_date_runwrapper" Name=turtleTask2 Dest=eccc-ppp3 CPUS=1 SYSTEM_ID="${JOB_ID:-${LOADL_STEP_ID%.*}}"
echo turtleTask2:5: JOB_STARTED at "$ORDSOUMET_date_started" Name=turtleTask2 Dest=eccc-ppp3 CPUS=1 SYSTEM_ID="${JOB_ID:-${LOADL_STEP_ID%.*}}"

export JobStartTime=$(date +%s -d "$ORDSOUMET_date_started")
export JobTimeLimit=300
export OMP_NUM_THREADS=1

# temporaire tant que je n'aurai pas trouvé de solution homogène...
if [[ "PBSPro" != "LoadLeveler5" ]]; then
	((OMP_NUM_THREADS=OMP_NUM_THREADS*1))
fi
export BATCH_MPI_CPUS=1

if [ -n "" ]; then
	export DISPLAY=
fi
if [ -n "" ]; then
	xterm -ls &
fi
sleep 1
nodelogger -n ${SEQ_NODE} ${SEQ_LOOP_ARGS} -s info -m "turtleTask2 nodelogger"
sleep 3

set +x
ORDSOUMET_date_ended="$(LC_TIME=C date)"
echo turtleTask2:6: JOB_ENDED at "$ORDSOUMET_date_ended" WallTime=$(echo  $(date +%s -d "$ORDSOUMET_date_ended") - $JobStartTime | bc)s Name=turtleTask2 Dest=eccc-ppp3 CPUS=1 SYSTEM_ID="${JOB_ID:-${LOADL_STEP_ID%.*}}"
