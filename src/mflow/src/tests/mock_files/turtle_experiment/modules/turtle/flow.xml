<MODULE name="turtle">
    <SUBMITS sub_name="turtleTask1"/>
    <SUBMITS sub_name="TurtlePower"/>
    <SUBMITS sub_name="turtleSlowpoke"/>
    <SUBMITS sub_name="turtleMemory"/>
    <TASK name="turtleTask1">
        <SUBMITS sub_name="turtleTask2"/>
    </TASK>
    <TASK name="turtleTask2"/>
    <LOOP name="TurtlePower">
        <SUBMITS sub_name="shredderTask"/>
        <SUBMITS sub_name="splinterTask"/>
        <SUBMITS sub_name="BossaNova"/>
        <SUBMITS sub_name="pizza1"/>
        <TASK name="shredderTask"/>
        <TASK name="splinterTask"/>
        <LOOP name="BossaNova">
            <SUBMITS sub_name="donatello"/>
            <TASK name="donatello"/>
        </LOOP>
        <TASK name="pizza1">
            <SUBMITS sub_name="pizza2" type="user"/>
        </TASK>
        <NPASS_TASK name="pizza2">
            <SUBMITS sub_name="pizza3" type="user"/>
        </NPASS_TASK>
        <NPASS_TASK name="pizza3"/>
    </LOOP>
    <TASK name="turtleSlowpoke"/>
    <TASK name="turtleMemory"/>
</MODULE>

