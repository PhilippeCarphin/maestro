export ORDSOUMET_date_started="$(LC_TIME=C date)"
export ORDSOUMET_date_submitted="Fri Apr 17 18:10:08 UTC 2020"
export ORDSOUMET_date_begin_ordsoumet="Fri Apr 17 18:10:08 UTC 2020"
export ORDSOUMET_date_prepared="Fri Apr 17 18:10:08 UTC 2020"
export ORDSOUMET_date_submit_done=""

echo turtleMemory:0: JOB_ORDSOUMET at "$ORDSOUMET_date_begin_ordsoumet" Name=turtleMemory Dest=eccc-ppp3 CPUS=1 SYSTEM_ID="${JOB_ID:-${LOADL_STEP_ID%.*}}"
echo turtleMemory:1: JOB_PREPARED at "$ORDSOUMET_date_prepared" Name=turtleMemory Dest=eccc-ppp3 CPUS=1 SYSTEM_ID="${JOB_ID:-${LOADL_STEP_ID%.*}}"
echo turtleMemory:2: JOB_SUBMITTED at "$ORDSOUMET_date_submitted" Name=turtleMemory Dest=eccc-ppp3 CPUS=1 SYSTEM_ID="${JOB_ID:-${LOADL_STEP_ID%.*}}"
echo turtleMemory:3: JOB_SUBMIT_DONE at "$ORDSOUMET_date_submit_done" Name=turtleMemory Dest=eccc-ppp3 CPUS=1 SYSTEM_ID=""
echo turtleMemory:4: JOB_RUNWRAPPER at "$ORDSOUMET_date_runwrapper" Name=turtleMemory Dest=eccc-ppp3 CPUS=1 SYSTEM_ID="${JOB_ID:-${LOADL_STEP_ID%.*}}"
echo turtleMemory:5: JOB_STARTED at "$ORDSOUMET_date_started" Name=turtleMemory Dest=eccc-ppp3 CPUS=1 SYSTEM_ID="${JOB_ID:-${LOADL_STEP_ID%.*}}"

export JobStartTime=$(date +%s -d "$ORDSOUMET_date_started")
export JobTimeLimit=300
export OMP_NUM_THREADS=1

# temporaire tant que je n'aurai pas trouvé de solution homogène...
if [[ "PBSPro" != "LoadLeveler5" ]]; then
	((OMP_NUM_THREADS=OMP_NUM_THREADS*1))
fi
export BATCH_MPI_CPUS=1

if [ -n "" ]; then
	export DISPLAY=
fi
if [ -n "" ]; then
	xterm -ls &
fi
python3 ${SEQ_EXP_HOME}/turtle_memory.py


set +x
ORDSOUMET_date_ended="$(LC_TIME=C date)"
echo turtleMemory:6: JOB_ENDED at "$ORDSOUMET_date_ended" WallTime=$(echo  $(date +%s -d "$ORDSOUMET_date_ended") - $JobStartTime | bc)s Name=turtleMemory Dest=eccc-ppp3 CPUS=1 SYSTEM_ID="${JOB_ID:-${LOADL_STEP_ID%.*}}"
